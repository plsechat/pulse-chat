{
  "name": "@pulse/server",
  "version": "0.1.1",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch ./src/index.ts",
    "dev:debug": "DEBUG=* bun --watch ./src/index.ts",
    "build": "tsc -b && bun ./build/build.ts",
    "db:gen": "bunx drizzle-kit generate --dialect postgresql --schema ./src/db/schema.ts --out ./src/db/migrations",
    "format": "prettier --write \"**/*.ts*\" --config ./.prettierrc.json",
    "lint": "eslint ./src/**/*.ts --ext ts --report-unused-disable-directives",
    "lint:fix": "eslint ./src/**/*.ts --ext ts --report-unused-disable-directives --fix",
    "check-types": "tsc --noEmit",
    "magic": "bun --bun run lint:fix && bun --bun run format && bun --bun run check-types",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "bun-types": "latest",
    "@types/ini": "^4.1.1",
    "@types/ws": "^8.18.1",
    "@types/semver": "^7.7.1",
    "@types/yauzl": "^2.10.3",
    "@types/yazl": "^3.3.0",
    "drizzle-kit": "^0.31.5",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^8.46.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@pulse/plugin-sdk": "workspace:*",
    "@pulse/shared": "workspace:*",
    "@supabase/supabase-js": "^2.49.1",
    "@trpc/server": "^11.6.0",
    "bun-sfe-autoupdater": "^0.0.4",
    "chalk": "^5.6.2",
    "cron": "^4.4.0",
    "drizzle-orm": "^0.44.5",
    "ini": "^5.0.0",
    "jose": "^6.0.11",
    "link-preview-js": "^3.1.0",
    "mediasoup": "^3.19.11",
    "postgres": "^3.4.5",
    "queue": "^7.0.0",
    "semver": "^7.7.3",
    "string-argv": "^0.3.2",
    "systeminformation": "^5.27.11",
    "ua-parser-js": "^2.0.6",
    "winston": "^3.17.0",
    "ws": "^8.18.3",
    "yauzl": "^3.2.0",
    "yazl": "^3.3.1",
    "zod": "^4.1.11"
  }
}